{% extends 'base.html.twig' %}

{% block contenu %}
    <h1>Etablissement</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etablissement.id }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ etablissement.ville }}</td>
            </tr>
            <tr>
                <th>Nomfrancais</th>
                <td>{{ etablissement.nomFrancais }}</td>
            </tr>
            <tr>
                <th>Nomanglais</th>
                <td>{{ etablissement.nomAnglais }}</td>
            </tr>
            <tr>
                <th>Devisefrancais</th>
                <td>{{ etablissement.deviseFrancais }}</td>
            </tr>
            <tr>
                <th>Deviseanglais</th>
                <td>{{ etablissement.deviseAnglais }}</td>
            </tr>
            <tr>
                <th>Boitepostal</th>
                <td>{{ etablissement.boitePostal }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('etablissement_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('etablissement_edit', { 'id': etablissement.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
