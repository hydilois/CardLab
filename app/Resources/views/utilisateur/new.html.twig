{% extends 'base.html.twig' %}
{% trans_default_domain 'FOSUserBundle' %}
{% block body %}
    <h1>Utilisateur creation</h1>
    <div class=" col-md-offset-3 col-md-6">
    {{ form_start(form, {'method': 'post', 'action': path('enseignant_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}
        {{ form_widget(form) }}
        <div><br>
            <input class="btn btn-primary" type="submit" value="{{ 'registration.submit'|trans }}" />
            <a class="btn btn-default" href="{{path('utilisateur_index')}}">Annuler</a>
        </div>
    {{ form_end(form) }}
</div>
{% endblock %}
